    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "rec = requests.get('http://news.163.com/')               \n",
    "rec.encoding = 'utf-8'#获取网页\n",
    "soup = BeautifulSoup(rec.text, 'html.parser')   #BeautifulSoup解析\n",
    "\n",
    "for news in soup.select('.data_row news_article clearfix '):\n",
    "    print news.text\n"
   ]
